<?php

/**
 * @file
 * Falcon_common.install file.
 */

/**
 * Implements hook_install().
 */
function falcon_common_install() {
  // Create default consumer.
  $consumer = \Drupal::entityTypeManager()->getStorage('consumer')->create([
    'type' => 'consumer',
    'label' => 'Default',
  ]);
  $consumer->save();
}
